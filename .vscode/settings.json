{
  "cSpell.words": [
    "AABB",
    "bezier",
    "bmesh",
    "bsdf",
    "Bsdf",
    "BSDF",
    "depsgraph",
    "EEVEE",
    "euler",
    "fcurve",
    "fcurves",
    "importldraw",
    "ldraw",
    "LDRAW",
    "lego",
    "Liftarm",
    "mathutils",
    "ptcache",
    "rigidbody",
    "subfolders",
    "substeps",
    "vdir",
    "verts",
    "vstep"
  ],
  "files.associations": {
    "*.py": "python"
  },
  "python.analysis.typeCheckingMode": "basic",
  "python.analysis.extraPaths": ["./blender", "./utils"],
  // No custom stubPath; use site-packages (fake-bpy-module) and typeshed
  "python.analysis.autoImportCompletions": true,
  "python.analysis.completeFunctionParens": true,
  "python.analysis.diagnosticMode": "workspace",
  "python.analysis.diagnosticSeverityOverrides": {
    "reportMissingImports": "warning",
    "reportMissingModuleSource": "warning"
  },
  "python.analysis.ignore": [
    "**/node_modules/**",
    "**/.git/**"
  ],
  "python.analysis.useLibraryCodeForTypes": false,
  // Use the workspace interpreter that has fake-bpy-module installed
  "python.defaultInterpreterPath": "/Users/sebastian/.pyenv/versions/3.13.5/bin/python",
  "python.linting.enabled": true,
  "python.linting.flake8Args": [
    "--ignore=E501,W503,F401"
  ],
  "python.linting.flake8Enabled": true,
  "python.linting.pylintEnabled": false
}